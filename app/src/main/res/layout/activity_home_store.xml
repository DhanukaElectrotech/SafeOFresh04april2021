                        android:weightSum="2">

                        <androidx.cardview.widget.CardView
                            android:id="@+id/supplierordercard"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_margin="1dp"
                            android:layout_weight="1"
                            android:background="@drawable/border_vendor"
                            app:cardCornerRadius="6dp"
                            app:cardUseCompatPadding="true"
                            card_view:cardElevation="5dp">

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_margin="@dimen/margin2dp"
                                android:onClick="openCustomerView"
                                android:orientation="vertical">

                                <ImageView
                                    android:id="@+id/supplierorderclk"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"

                                    android:src="@drawable/userview" />

                                <TextView

                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentBottom="true"
                                    android:background="#3772a8"
                                    android:gravity="center"
                                    android:text="Supplier Order"
                                    android:textAllCaps="true"
                                    android:textColor="#ffffff"
                                    android:textStyle="bold" />


                            </RelativeLayout>
                        </androidx.cardview.widget.CardView>

                    </LinearLayout>


                </LinearLayout>


            </LinearLayout>
        </ScrollView>


    </RelativeLayout>
</LinearLayout>