                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_margin="1dp"
                            android:layout_weight="1"
                            android:background="@drawable/border_vendor"
                            app:cardCornerRadius="6dp"
                            app:cardUseCompatPadding="true"
                            card_view:cardElevation="5dp">

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_margin="@dimen/margin2dp"
                                android:orientation="vertical">

                                <ImageView
                                    android:id="@+id/purchaseordrclk"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_above="@+id/PO"
                                    android:onClick="openGr"
                                    android:padding="4dp"
                                    android:src="@drawable/sales" />

                                <TextView

                                    android:id="@+id/PO"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentBottom="true"
                                    android:background="#3772a8"
                                    android:gravity="center"
                                    android:text="Purchase Order"
                                    android:textAllCaps="true"
                                    android:textColor="#ffffff"
                                    android:textStyle="bold" />


                            </RelativeLayout>

                        </androidx.cardview.widget.CardView>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="190dp"
                        android:orientation="horizontal"
                        android:weightSum="2">

                        <androidx.cardview.widget.CardView
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_margin="1dp"
                            android:layout_weight="1"
                            android:background="@drawable/border_vendor"
                            app:cardCornerRadius="6dp"
                            app:cardUseCompatPadding="true"
                            card_view:cardElevation="5dp">

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_margin="@dimen/margin2dp"
                                android:onClick="openCreateGR"
                                android:orientation="vertical">

                                <ImageView
                                    android:layout_width="100dp"
                                    android:layout_height="100dp"
                                    android:layout_centerInParent="true"
                                    android:background="@drawable/circle"
                                    android:onClick="openCreateGR"
                                    android:padding="10dp"
                                    android:src="@drawable/ic_edit" />

                                <TextView

                                    android:id="@+id/txt45"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentBottom="true"
                                    android:background="#3772a8"
                                    android:gravity="center"
                                    android:text="Create GR"
                                    android:textAllCaps="true"
                                    android:textColor="#ffffff"
                                    android:textStyle="bold" />


                            </RelativeLayout>
                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_margin="1dp"
                            android:layout_weight="1"
                            android:background="@drawable/border_vendor"
                            app:cardCornerRadius="6dp"
                            app:cardUseCompatPadding="true"
                            card_view:cardElevation="5dp">

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_margin="@dimen/margin2dp"
                                android:orientation="vertical">

                                <ImageView
                                    android:id="@+id/storewiseclick"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:onClick="openNewstorewise"

                                    android:padding="14dp"
                                    android:src="@drawable/sales" />

                                <TextView

                                    android:id="@+id/storewisec"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentBottom="true"
                                    android:background="#3772a8"
                                    android:gravity="center"
                                    android:text="Store Wise Stock"
                                    android:textAllCaps="true"
                                    android:textColor="#ffffff"
                                    android:textStyle="bold" />


                            </RelativeLayout>

                        </androidx.cardview.widget.CardView>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="190dp"
                        android:orientation="horizontal"
                        android:weightSum="2">

                        <androidx.cardview.widget.CardView
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_margin="1dp"
                            android:layout_weight="1"
                            android:background="@drawable/border_vendor"
                            android:visibility="gone"
                            app:cardCornerRadius="6dp"
                            app:cardUseCompatPadding="true"
                            card_view:cardElevation="5dp">

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_margin="@dimen/margin2dp"
                                android:orientation="vertical">

                                <ImageView
                                    android:layout_width="100dp"
                                    android:layout_height="100dp"
                                    android:layout_centerInParent="true"
                                    android:background="@drawable/circle"

                                    android:onClick="openNewstorewise"
                                    android:padding="10dp"
                                    android:src="@drawable/ic_edit" />

                                <TextView

                                    android:id="@+id/txtnewstorewisestck"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentBottom="true"
                                    android:background="#3772a8"
                                    android:gravity="center"
                                    android:text="Sttore wise stock new"
                                    android:textAllCaps="true"
                                    android:textColor="#ffffff"
                                    android:textStyle="bold" />


                            </RelativeLayout>
                        </androidx.cardview.widget.CardView>


                        <androidx.cardview.widget.CardView
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_margin="1dp"
                            android:layout_weight="1"
                            android:background="@drawable/border_vendor"
                            app:cardCornerRadius="6dp"
                            app:cardUseCompatPadding="true"
                            card_view:cardElevation="5dp">

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_margin="@dimen/margin2dp"
                                android:orientation="vertical">

                                <ImageView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:onClick="opennewbilling"

                                    android:padding="14dp"
                                    android:src="@drawable/sales" />

                                <TextView

                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentBottom="true"
                                    android:background="#3772a8"
                                    android:gravity="center"
                                    android:text="Billing"
                                    android:textAllCaps="true"
                                    android:textColor="#ffffff"
                                    android:textStyle="bold" />


                            </RelativeLayout>

                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
