<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical"

    tools:context="com.dhanuka.morningparcel.activity.HomeStoreActivity">


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        android:orientation="vertical">

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/white"
            android:paddingTop="5dp"
            android:scrollbars="none">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/white"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/white"
                    android:orientation="vertical"
                    android:paddingLeft="10dp"
                    android:paddingRight="10dp"
                    android:paddingBottom="10dp">

                    <com.dhanuka.morningparcel.customViews.KKViewPager
                        android:id="@+id/kk_pager"
                        android:layout_width="match_parent"
                        android:layout_height="180dp"

                        android:layout_alignParentStart="true"
                        android:layout_alignParentLeft="true" />

                    <com.tbuonomo.viewpagerdotsindicator.DotsIndicator
                        android:id="@+id/dots_indicate"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginTop="10dp"
                        android:layout_marginBottom="10dp"
                        app:dotsColor="#000000"
                        app:dotsCornerRadius="8dp"
                        app:dotsSize="8dp"
                        app:dotsSpacing="4dp"
                        app:dotsWidthFactor="2.5"
                        app:progressMode="true"
                        app:selectedDotColor="@color/colorAccent" />
                </LinearLayout>

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="5dp"
                    android:translationZ="5dp"
                    android:visibility="visible">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="44dp"
                        android:gravity="center_vertical"

                        android:orientation="horizontal"
                        android:paddingLeft="10dp"
                        android:paddingRight="10dp"

                        >

                        <AutoCompleteTextView
                            android:id="@+id/etSearch"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_marginRight="40dp"
                            android:background="#ffffff"
                            android:ems="10"
                            android:visibility="gone" />

                        <TextView
                            android:id="@+id/etSearch1"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_marginRight="40dp"
                            android:background="#ffffff"
                            android:ems="10"
                            android:gravity="center_vertical"
                            android:text="Search Customer" />

                        <ImageView
                            android:id="@+id/searhlist"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_marginLeft="-30dp"
                            android:src="@drawable/ic_search_black_24dp" />


                    </LinearLayout>

                </androidx.cardview.widget.CardView>

                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/dashlistcontainer"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />


                <LinearLayout
                    android:id="@+id/categorylayout"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"

                    android:orientation="vertical"
                    android:visibility="gone">


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="190dp">

                        <androidx.cardview.widget.CardView
                            android:id="@+id/card1"
                            android:layout_width="wrap_content"
                            android:layout_height="190dp"
                            android:layout_margin="1dp"
                            android:layout_weight="1"
                            android:background="@drawable/border_vendor"
                            app:cardCornerRadius="6dp"
                            app:cardUseCompatPadding="true"
                            card_view:cardElevation="5dp">

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_margin="@dimen/margin2dp"
                                android:orientation="vertical">

                                <ImageView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_above="@+id/txt1"
                                    android:onClick="openProducts"
                                    android:scaleType="centerCrop"
                                    android:src="@drawable/product_grocery" />

                                <TextView
                                    android:id="@+id/txt1"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentBottom="true"

                                    android:background="#3772a8"
                                    android:gravity="center"
                                    android:minHeight="20dp"
                                    android:text="Products"
                                    android:textAllCaps="true"
                                    android:textColor="#ffffff"
                                    android:textStyle="bold" />


                            </RelativeLayout>
                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:id="@+id/card2"
                            android:layout_width="wrap_content"
                            android:layout_height="190dp"
                            android:layout_margin="1dp"
                            android:layout_toLeftOf="@+id/card1"
                            android:layout_weight="1"
                            android:background="@drawable/border_vendor"
                            app:cardCornerRadius="6dp"
                            app:cardUseCompatPadding="true"
                            card_view:cardElevation="5dp">

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_margin="@dimen/margin2dp"
                                android:orientation="vertical">

                                <ImageView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_above="@+id/txt2"
                                    android:onClick="openOrders"
                                    android:scaleType="centerCrop"
                                    android:src="@drawable/orders_grocery" />

                                <TextView
                                    android:id="@+id/txt2"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentBottom="true"

                                    android:background="#3772a8"
                                    android:gravity="center"
                                    android:text="Orders"
                                    android:textAllCaps="true"
                                    android:textColor="#ffffff"
                                    android:textStyle="bold" />


                            </RelativeLayout>
                        </androidx.cardview.widget.CardView>

                    </LinearLayout>


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="190dp"
                        android:orientation="horizontal"
                        android:weightSum="2">

                        <androidx.cardview.widget.CardView
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_margin="1dp"
                            android:layout_weight="1"
                            android:background="@drawable/border_vendor"
                            app:cardCornerRadius="6dp"
                            app:cardUseCompatPadding="true"
                            card_view:cardElevation="5dp">

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_margin="@dimen/margin2dp"
                                android:orientation="vertical">

                                <ImageView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_above="@+id/txt44"
                                    android:onClick="openSales"
                                    android:padding="4dp"
                                    android:src="@drawable/sales" />

                                <TextView

                                    android:id="@+id/txt44"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentBottom="true"
                                    android:background="#3772a8"
                                    android:gravity="center"
                                    android:text="Sale Reports"
                                    android:textAllCaps="true"
                                    android:textColor="#ffffff"
                                    android:textStyle="bold" />


                            </RelativeLayout>
                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:id="@+id/pendingcard"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_margin="1dp"
                            android:layout_weight="1"
                            android:background="@drawable/border_vendor"
                            app:cardCornerRadius="6dp"
                            app:cardUseCompatPadding="true"
                            card_view:cardElevation="5dp">

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_margin="@dimen/margin2dp"
                                android:orientation="vertical">

                                <ImageView
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_above="@+id/txt4"
                                    android:onClick="openItems"
                                    android:src="@drawable/about_grocery" />

                                <TextView

                                    android:id="@+id/txt4"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_alignParentBottom="true"
                                    android:background="#3772a8"
                                    android:gravity="center"
                                    android:text="Pending Items"
                                    android:textAllCaps="true"
                                    android:textColor="#ffffff"
                                    android:textStyle="bold" />


                            </RelativeLayout>
                        </androidx.cardview.widget.CardView>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="190dp"
                        android:orientation="horizontal"
                        android:weightSum="2">

                        <androidx.cardview.widget.CardView
                            android:id="@+id/profilecard"
